      <div name="<%= name %>" controller="monscape" class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span name = "<%= name %>name" class="card-title"></span>
          <span name = "<%= name %>win" ></span>
          <a class='btn-floating halfway-fab waves-effect waves-light blue-grey darken-2 dropdown-trigger' href='#' data-target='dropdown<%= name %>'><i class="material-icons">add</i></a>
          <!-- Dropdown Trigger -->

          <p name="<%= name %>input"></p>
          <p class="range-field">

            <input type="range" name="<%= name %>R" min="0" max="100" value ="0"/>
            <input type="range" name="<%= name %>V" min="0" max="100" value ="0"/>
            <input type="range" name="<%= name %>B" min="0" max="100" value ="0"/>
          </p>
        </div>
        <div class="card-action">
          <span>Dernier essai : </span>
          <span name="<%= name %>Last_input"> </span>
        </div>
      </div>